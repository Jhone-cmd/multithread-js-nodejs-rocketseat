{
  "$schema": "https://biomejs.dev/schemas/2.1.2/schema.json",
  "extends": ["ultracite"],
  "javascript": {
    "formatter": {
      "semicolons": "asNeeded"
    }
  },

  "linter": {
    "rules": {
      "suspicious": {
        "noConsole": "off"
      },
      "complexity": {
        "noForEach": "off"
      },
      "nursery": {
        "useIterableCallbackReturn": "off"
      }
    }
  }
}
